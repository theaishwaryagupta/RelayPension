<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.codewarriors.hackathone.relaypension.adminside.listallconspack.ListAllConstituency"
    android:background="@color/avtivitybackground"
    android:padding="7dp">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentEnd="true"
        android:background="@drawable/layoutbackground"
        android:dividerPadding="10dp"
        android:padding="2dp"
        android:orientation="vertical">

        <ListView
            android:id="@+id/listviewallconstituency"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:dividerHeight="3dp"
            android:divider="@color/avtivitybackground"
            android:fastScrollAlwaysVisible="false"
            android:fastScrollEnabled="true" />
    </LinearLayout>

</RelativeLayout>
